{
  "name": "logtify",
  "version": "1.1.3",
  "description": "Logger based on the Chain of Responsibility pattern",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false ./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/_mocha test --recursive --timeout=10000 --exit",
    "test": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-logtify.git"
  },
  "author": "Dial Once",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dial-once/node-logtify/issues"
  },
  "homepage": "https://github.com/dial-once/node-logtify#readme",
  "dependencies": {
    "circular-json": "^0.4.0",
    "deep-freeze": "0.0.1",
    "serialize-error": "^2.1.0",
    "sinon": "^1.17.6",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  }
}
