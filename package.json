{
  "name": "logtify",
  "version": "1.1.3",
  "description": "Logger based on the Chain of Responsibility pattern",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- test --recursive --timeout=20000 --exit",
    "test": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dial-once/node-logtify.git"
  },
  "author": "Dial Once",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dial-once/node-logtify/issues"
  },
  "homepage": "https://github.com/dial-once/node-logtify#readme",
  "dependencies": {
    "circular-json": "^0.4.0",
    "deep-freeze": "0.0.1",
    "serialize-error": "^2.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "sinon": "^1.17.6"
  }
}
